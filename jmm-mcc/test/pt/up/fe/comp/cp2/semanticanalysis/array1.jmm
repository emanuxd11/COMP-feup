import io;

class Simple {

    public int updatePos(int pos, Simple s) {
        int[] arr;
        int aux;

        arr = new int[10];
        arr[1] = 3;

        aux = arr[pos];

        arr[pos + 1] = arr[pos-1] * 3 ;

        return aux;
    }


    public static void main(String[] args) {

        int val;

        Simple s;
        s = new Simple();

        val = s.updatePos(5, s);

        io.println(val); // 3

       
    }


}

